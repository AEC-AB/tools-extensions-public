<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Configurations>Debug;Debug 2020;Debug 2021;Debug 2022;Debug 2023;Debug 2024;Release
      2020;Release 2021;Release 2022;Release 2023;Release 2024</Configurations>
    <ExtensionId>SelectObjectFromSelectionFilter</ExtensionId>
    <Version>0.0.3</Version>
    <Title>SelectObjectFromSelectionFilter</Title>
    <Description>Select objects in a Tekla model based on filter</Description>
    <PackageTags>Tekla;Assistant;Extension</PackageTags>
    <PackageIcon>Icon.png</PackageIcon>
    <MainTeklaVersion Condition="'$(Configuration)' == 'Debug'">2024</MainTeklaVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Icon.png" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>